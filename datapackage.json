          },
          {
            "name": "town",
            "type": "string",
            "title": "町域名"
          },
          {
            "name": "multi_zipcode",
            "type": "boolean",
            "title": "一町域が二以上の郵便番号で表される場合の表示"
          },
          {
            "name": "koaza_split",
            "type": "boolean",
            "title": "小字毎に番地が起番されている町域の表示"
          },
          {
            "name": "choume_view",
            "type": "boolean",
            "title": "丁目を有する町域の場合の表示"
